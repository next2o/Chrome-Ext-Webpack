{
    "short_name": "Next2O",
    "name": "Next2O",
    "version": "0.1",
    "manifest_version": 3,
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "index.html",
      "default_title": "open popup"
    },
    "permissions": [
      "activeTab",
      "scripting",
      "background",
      "tabs"
    ]
    
  }
  